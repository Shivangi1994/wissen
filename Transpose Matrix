class Solution {
    public int[][] transpose(int[][] matrix) {
        int R = matrix.length;
        int C = matrix[0].length;
        int result[][] = new int[C][R];
        for(int row=0; row< R; row++){
            for(int col=0;col < C; col++){
                result[col][row] = matrix[row][col];
            }
        }
    return result;
    }
}
